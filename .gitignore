# Create .gitignore in root
cat > .gitignore << 'EOF'
# Dependencies
node_modules/
frontend/node_modules/

# Environment variables
.env
.env.local
.env.production

# Logs
logs/
*.log
npm-debug.log*

# Build outputs
dist/
build/
.next/
frontend/.next/
frontend/out/


# OS files
.DS_Store
Thumbs.db

# IDE
.vscode/
.idea/
*.swp
*.swo
